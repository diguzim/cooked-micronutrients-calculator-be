datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model RawIngredient {
  id                  Int      @id @default(autoincrement())
  name                String
  proteinRatio       Float
  fatRatio           Float
  carbohidrateRatio  Float
  fiberRatio         Float
  kcalPerGram       Float
}